{
  "name": "calendar",
  "version": "1.0.0",
  "description": "Calendar Application",
  "main": "index.js",
  "scripts": {
    "install-server": "cd src/Server && npm install",
    "install-client": "cd src/Client && npm install",
    "start-server": "cd src/Server && npm start",
    "start-client": "cd src/Client && npm start",
    "server": "cd src/Server && npm run dev",
    "client": "cd src/Client && npm start",
    "build": "cd src/Client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && npm run install-server && npm run install-client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}